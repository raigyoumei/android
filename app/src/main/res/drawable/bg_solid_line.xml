<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <solid
                android:color="#EFEFEF"/>
            <corners
                android:radius="10dip"
                />
            <stroke
                android:dashGap="0dp"
                android:dashWidth="1dp"
                android:width="1dp"
                android:color="#8B8B8B" />
        </shape>
    </item>
</layer-list>

